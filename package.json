{
  "name": "media-manager-service",
  "version": "1.0.0",
  "description": "Media manager API service used to handle files",
  "main": "app.ts",
  "author": "Franco E. Vitelli",
  "license": "ISC",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "NODE_ENV=dev && nodemon src/app.ts",
    "local": "docker-compose up -d && NODE_ENV=local nodemon src/app.ts",
    "start": "NODE_ENV=production && node ./dist/src/app.js",
    "test:integration": "npm run test:integration:up && NODE_ENV=test jest --config jest.integration.config.cjs --verbose --detectOpenHandles && npm run test:integration:down",
    "test:integration:down": "docker-compose -f test/docker-compose.yml down -v",
    "test:integration:up": "docker-compose -f test/docker-compose.yml up -d",
    "test:unit": "jest --verbose",
    "deploy:prod": "sst deploy --stage production"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.844.0",
    "@aws-sdk/s3-request-presigner": "^3.844.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "install": "^0.13.0",
    "kysely": "^0.28.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "npm": "^11.4.2",
    "pg": "^8.16.3",
    "sst": "3.17.10",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@types/aws-lambda": "8.10.150",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "babel-jest": "^30.0.4",
    "dotenv-cli": "^8.0.0",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
